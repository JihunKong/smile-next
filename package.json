{
  "name": "smile-next",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "predev": "npm run dev:stop --silent",
    "dev": "next dev --turbopack",
    "predev:simple": "npm run dev:stop --silent",
    "dev:simple": "DISABLE_WORKERS=true next dev --turbopack",
    "dev:stop": "lsof -ti:3000,3001 | xargs kill -9 2>/dev/null || true",
    "build": "prisma generate && next build",
    "start": "next start",
    "setup": "./scripts/dev/setup-local.sh",
    "setup:reset": "./scripts/dev/setup-local.sh --reset",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:check": "eslint . --max-warnings 0",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "npx tsx prisma/seed.ts",
    "db:reset": "./scripts/dev/reset-db.sh",
    "postinstall": "prisma generate",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test:unit && npm run test:e2e",
    "test:ci": "npm run lint:check && npm run type-check && npm run test:unit && npm run test:e2e",
    "workers": "npx tsx scripts/start-workers.ts"
  },
  "prisma": {
    "seed": "npx tsx prisma/seed.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@auth/prisma-adapter": "^2.11.1",
    "@prisma/client": "^6.19.1",
    "@tanstack/react-query": "^5.90.16",
    "@types/qrcode": "^1.5.6",
    "bcryptjs": "^3.0.3",
    "bull": "^4.16.5",
    "date-fns": "^4.1.0",
    "ioredis": "^5.8.2",
    "jose": "^5.9.6",
    "lock": "^1.1.0",
    "next": "16.1.1",
    "next-auth": "^5.0.0-beta.30",
    "next-intl": "^4.7.0",
    "nodemailer": "^7.0.12",
    "nvm": "^0.0.4",
    "openai": "^6.15.0",
    "prisma": "^6.19.1",
    "puppeteer": "^24.34.0",
    "qrcode": "^1.5.4",
    "qrcode.react": "^4.2.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "stripe": "^20.1.2",
    "uuid": "^13.0.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.1.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/nodemailer": "^7.0.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "eslint-config-prettier": "^10.1.8",
    "jsdom": "^25.0.1",
    "prettier": "^3.7.4",
    "tailwindcss": "^4",
    "tsx": "^4.7.0",
    "typescript": "^5",
    "vitest": "^2.1.8"
  }
}
